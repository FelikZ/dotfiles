export LSCOLORS=cxGxBxDxCxEgEdxbxgxccA
export PS1="\[\033[36m\]\u\[\033[m\]@\[\033[34m\]\h:\[\033[0;36m\]\w\[\033[m\] \[\033[0;37m\]\$ "
